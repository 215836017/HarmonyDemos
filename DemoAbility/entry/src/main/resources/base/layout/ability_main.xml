<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical">

    <Text
        ohos:id="$+id:main_1_text"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:background_element="$graphic:background_ability_main"
        ohos:layout_alignment="horizontal_center"
        ohos:text="入口Ability的入口AbilitySlice"
        ohos:text_size="20fp"
        />

    <Button
        ohos:id="$+id:main_1_btn_1"
        ohos:height="30vp"
        ohos:width="match_parent"
        ohos:background_element="#ff009988"
        ohos:margin="10vp"
        ohos:text="同一Page Ability中跳转AbilitySlice"
        ohos:text_size="18fp"/>

    <Button
        ohos:id="$+id:main_1_btn_2"
        ohos:height="30vp"
        ohos:width="match_parent"
        ohos:background_element="#ff009988"
        ohos:margin="10vp"
        ohos:text="同一Page Ability中跳转AbilitySlice并返回结果"
        ohos:text_size="15fp"/>

</DirectionalLayout>